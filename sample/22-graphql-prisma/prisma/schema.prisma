// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

datasource db {
  provider = "sqlite"
}

generator client {
  provider     = "prisma-client-js"
  moduleFormat = "cjs"
}

model Post {
  id          String  @id @default(uuid())
  title       String
  text        String
  isPublished Boolean @default(false)
}
